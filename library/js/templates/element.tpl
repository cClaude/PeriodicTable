<div class="element" data-id="{{=it.symbol}}" data-magnetic-order="{{=it.mag.type || 'unknown'}}" data-state="{{=it.state.type || 'unknown'}}">
    <div class="inner">
        <div class="number">{{=it.number}}</div>
        <abbr title="{{=it.name}}" class="symbol">{{=it.symbol}}</abbr>
        <div class="details">
            {{=it.name}}
        </div>
    </div>
</div>